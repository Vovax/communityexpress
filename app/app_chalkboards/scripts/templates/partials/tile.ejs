<a href="#" >
    <div class="cmntyex_sasl_tile">
        <% if (showURL) { %>
            <div class="more_detailes">
                <a href="<%= onClickURL %>">
                    <h2>For more detailes click here</h2>
                </a>
            </div>
        <% } %>
        <% if (promoType.enumText == 'CAMPAIGN_PROMOTION') { %>
            <div class="create_promotion">
                <h2>Create Promotion</h2>
            </div>
        <% } %>
        <div class="tile_left">
            <h1 class="tile_name"><%= saslName %></h1>
            <h2 class="tile_type"><%= promoType.enumText %></h2>
            <img class="tile_rating" src="">
            <img class="tile_image" src="<%= url %>" >
        </div>
        <div class="tile_right">
            <h2 class="tile_title"><%= title %></h2>
            <p class="tile_message"><%= message %></p>
        </div>
    </div>
</a>
